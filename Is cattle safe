#include<iostream>
using namespace std;
int main()
{
    int x,y,x1=0,y1=0,d;
    cin>>d>>x>>y;
    for (int i=0;i<d;i++){
        for (int j=0;j<d;j++){
            if (i*i+j*j==d*d){
                if ((i-1>=x && j-1>=y) || ((d/1.41)-1>=x && (d/1.41)-1>=y)){
                cout<<"Safe";
                return 0;}
                else{
                cout<<"Unsafe";
                return 0;}
            }
        }
    }
}
